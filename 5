#include <stdio.h>

int main() {
    int sum = 0;
    int count = 0;
    int num;
    scanf("%d", &num);
    while (num != 0) {
        sum += num;
        count++;
        scanf("%d", &num);
    }
    if (count > 0) {
        printf("%.2f", (float)sum / count);
    } else {
        printf("No numbers entered.");
    }
    return 0;
}

