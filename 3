#include <stdio.h>
#include <string.h>
#include <ctype.h>

int has_uppercase(char* password) {
    for (int i = 0; i < strlen(password); i++) {
        if (isupper(password[i])) {
            return 1;
        }
    }
    return 0;
}

int has_lowercase(char* password) {
    for (int i = 0; i < strlen(password); i++) {
        if (islower(password[i])) {
            return 1;
        }
    }
    return 0;
}

int has_digit(char* password) {
    for (int i = 0; i < strlen(password); i++) {
        if (isdigit(password[i])) {
            return 1;
        }
    }
    return 0;
}

int has_special(char* password) {
    for (int i = 0; i < strlen(password); i++) {
        if (!isalnum(password[i])) {
            return 1;
        }
    }
    return 0;
}

int is_valid_password(char* password) {
    if (strlen(password) < 8 || strlen(password) > 14) {
        return 0;
    }
    if (!has_uppercase(password) || !has_lowercase(password) || !has_digit(password) || !has_special(password)) {
        return 0;
    }
    for (int i = 0; i < strlen(password); i++) {
        if (password[i] < 33 || password[i] > 126) {
            return 0;
        }
    }
    return 1;
}

int main() {
    char password[15];
    printf("Enter password: ");
    scanf("%s", password);
    if (is_valid_password(password)) {
        printf("Password is valid.\n");
    } else {
        printf("Password is invalid.\n");
    }
    return 0;
}

